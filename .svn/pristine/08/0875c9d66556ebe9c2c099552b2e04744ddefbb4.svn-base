function Package(){
	_extends(this, AbstractElement);
	//包定义头
	this.packageHeader = "";
	this.redefinableHeader;										//可重定义头


	this.conformanceClass;										//一致分类声明


	this.script;												//脚本
	
	this.externalPackages = new Array();				//外部包参考

	this.typeDeclarations = new Array();				//类型定义
	this.participants = new Array();						//参与者
	this.applications = new Array();						//应用程序声明
	this.procedures = new Array();							//过程声明
	this.dataFields = new Array();							//数据类型声明
	this.eventTypes = new Array();							//事件类型声明
	this.workflowProcesses = new Array();				//流程
}