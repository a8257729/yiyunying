* {
	padding: 0px;
	margin: 0px;
}

ul li {
	list-style: none;
}

#box_top {
	border-bottom: 1px solid gainsboro;
	width: 100%;
	height: 66px;
	line-height: 66px;
}

#top_title {
	border: 1px solid transparent;
	width: 100%;
	height: 24px;
	line-height: 24px;
	text-align: center;
	position: absolute;
	line-height: 24px;
	top: 16px;
	font-size: 18px;
}

#top_back {
	position: absolute;
	left: 2%;
}

#top_fresh {
	position: absolute;
	right: 2%;
}

#search_area {
	border: 1px solid transparent;
	height: 80px;
	text-align: center;
}

#search_case {
	border: 1px solid transparent;
	margin-top: 25px;
	text-align: center;
}

#search_area button {
	border: 1px solid transparent;
	width: 20%;
	height: 26.5px;
	background-color:#FFA500;/*#f36f2a*/
	color: white;
}

#search_input {
	border: 1px solid #00BFFF;
	width: 68%;
	height: 26px;
}

.rectangle_case {
	border: 1px solid transparent;
	width: auto;
	height: 25px;
	line-height: 25px;
	font-size: 12px;
	border-radius: 15px;
	text-align: center;
	background-color: #00BFFF;/*#1c80d5*/
	color: white;
	display: inline-block;
	*display: inline;
	*zoom: 1;
	padding:8px;
	margin: 8px 10px 5px 5px;
}

#box_middle {
	width: 90%;
	height: auto;
	border: 1px solid gainsboro;
	margin-left:5%;
}
