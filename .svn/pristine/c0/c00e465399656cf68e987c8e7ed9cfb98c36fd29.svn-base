* {
			padding: 0px;
			margin: 0px;
		}
		
		
		#box_top {
			border-bottom:1px solid gainsboro;
			width: 100%;
			height: 74px;
		}
		
		#top_title {
			border: 1px solid transparent;
			height: 24px;
			line-height: 24px;
			text-align: center;
			position: absolute;
			left: 40%;
			top: 28px;
			font-size: 18px;
		}
		#top_back {
			position: absolute;
			left: 2%;
			top: 36px;
		}
		.bg{
			width:100%;
			height:10px;
			background-color:#eceef3;
		}
		.port_info{
			border:1px solid transparent;
			width:100%;
			height:auto;
		}
		.port_up{
			border:1px solid transparent;
			width:auto;
			height:auto;
			margin-left:6%;
			margin-top:12px;
			font-size:14px;
			color:#f36f2a;
		}
		.port_down{
			border:1px solid transparent;
			width:auto;
			height:25px;
			line-height:25px;
			margin-left:6%;
			margin-top:1px;
			font-size:14px;
			color:#f36f2a;
		}
		.port_info span{
			color:#666666;
		}
		#fenye{
			border:1px solid transparent;height:40px;line-height:40px;text-align:center;
		}
		a {
			text-decoration:none;
			color:#0000FF;
		}