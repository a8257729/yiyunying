* {
			padding: 0px;
			margin: 0px;
}
ul li{
	list-style: none;
}
#box_top {
	border-bottom: 1px solid gainsboro;
	width: 100%;
	height: 66px;
	line-height:66px;
	
}

#top_title {
	border: 1px solid transparent;
	width:100%;
	height: 24px;
	line-height: 24px;
	text-align: center;
	position: absolute;
	line-height:24px;
	top:16px;
	font-size: 18px;
}

#top_back {
	position: absolute;
	left: 2%;
	
}

#top_fresh {
	position: absolute;
	right: 2%;
	
}
#search_area{
	border:1px solid transparent;
	height:80px;
	text-align: center;
	
}
#search_case{
	border:1px solid transparent;
	margin-top:10px;
	text-align: center;
	
}
/*#f36f2a*/
#search_area button{
	border:1px solid transparent;
	width:20%;
	height:26.5px;
	background-color:#00BFFF;
	color:white;
}
/*#FF0000*/
#search_input {
	border:1px solid #00BFFF;
	width:68%;
	height:26px;
	
}
#search_tip{
	border:1px solid transparent;
	font-size:12px;
	color:#999999;
	text-align:center;
	margin-top:6px;
	margin-left:5%;
}
#tip_area ul{
	margin-left:2.5%;
}
#search_content li{
	float: left;
	border:1px solid transparent;
	width:75%;
	margin-top:5px;
	
}
#search_content{
	border:1px solid transparent;
	
}
.name{
	font-size:14px;
	color:black;
	margin-right:8px;
	color:#999999;
}
#search_content span{
	font-size:14px;
}
#search_content div{
	border:1px solid transparent;
	margin-left:10%;
	margin-bottom:3px;
	padding:5px;
}
#split_bar{
	border:1px solid transparent;
	height:3px;
	width:100%;
	background:#eff2f5;
}
#result_tip{
	border:1px solid transparent;
	width:100%;
	height:38px;
	line-height:38px;
	font-size:18px;
	color:#333333;
	
}
.result_split_bar{
	border:1px solid transparent;
	height:1px;
	background-color: #eff2f5;
}

#bt{
	border:1px solid transparent;
	width:100%;
	height: auto;
	margin-top:10%;
	text-align: center;
}
#bt button{
	margin-left:3%;
	border-radius:10%;
	width:65px;
	height:40px;
	color:#EFF2F5;
}
#bt_create_childlock{
	background-color:#32CD32;
}
#bt_modify_childlock{
	background-color:#FFA500;
}
#bt_close_childlock{
	background-color:#FF0000;
}
#bt_reset_childlock{
	background-color:#00BFFF;
}
